<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= form_for @song  do |f| %>
    <%= f.label :artist_name %>
    <%= f.text_field :artist_name %>
    <br />
    <%= form_for @song  do |f| %>
      <%= f.label :genre_name%>
      <%= f.text_field :genre_name %>
      <br />
      <%= form_for @song  do |f| %>
        <%= f.label :note_contents%>
        <%= f.text_field :note_contents %>
        <br />

 <%= f.submit %>

<% end %>
